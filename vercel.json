{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task",
    "VERCEL": "1"
  },
  "functions": {
    "wsgi.py": {
      "maxDuration": 60,
      "memory": 3008
    }
  }
}
